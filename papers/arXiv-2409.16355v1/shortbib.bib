	eprint = {1408.3853},
	journal = {\pasp},
	keywords = {Data Analysis and Techniques},
	pages = {948-958},
	primaryclass = {astro-ph.IM},
	title = {{A Technique for Extracting Highly Precise Photometry for the Two-Wheeled Kepler Mission}},
