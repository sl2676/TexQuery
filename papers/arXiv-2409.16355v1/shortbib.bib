@article{Carter2011,
	abstract = {The Kepler spacecraft has been monitoring the light from 150,000 stars in its primary quest to detect transiting exoplanets. Here, we report on the detection of an eclipsing stellar hierarchical triple, identified in the Kepler photometry. KOI-126 [A, (B, C)], is composed of a low-mass binary [masses MB = 0.2413 $\pm$ 0.0030 solar mass (M&sun;), MC = 0.2127 $\pm$ 0.0026 M&sun;; radii RB = 0.2543 $\pm$ 0.0014 solar radius (R&sun;), RC = 0.2318 $\pm$ 0.0013 R&sun;; orbital period P1 = 1.76713 $\pm$ 0.00019 days] on an eccentric orbit about a third star (mass MA = 1.347 $\pm$ 0.032 M&sun;; radius RA = 2.0254 $\pm$ 0.0098 R&sun;; period of orbit around the low-mass binary P2 = 33.9214 $\pm$ 0.0013 days; eccentricity of that orbit e2 = 0.3043 $\pm$ 0.0024). The low-mass pair probe the poorly sampled fully convective stellar domain offering a crucial benchmark for theoretical stellar models.},
