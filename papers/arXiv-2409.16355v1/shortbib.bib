	eid = {15},
	eprint = {1703.05775},
	journal = {\apjs},
	keywords = {astrometry, binaries: close, brown dwarfs, fundamental parameters, parallaxes, stars: evolution},
	month = aug,
	pages = {15},
	primaryclass = {astro-ph.SR},
	title = {{Individual Dynamical Masses of Ultracool Dwarfs}},
