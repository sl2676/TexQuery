	journal = {\apj},
	keywords = {brown dwarfs, infrared: stars, planets and satellites: atmospheres, stars: low-mass},
	month = aug,
	pages = {79},
	primaryclass = {astro-ph.SR},
	title = {{A Near-infrared Spectroscopic Study of Young Field Ultracool Dwarfs}},
