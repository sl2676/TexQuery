	eid = {57},
	eprint = {1910.12988},
	journal = {\aj},
	keywords = {Astrophysics - Earth and Planetary Astrophysics},
	month = feb,
	number = {2},
	pages = {57},
	primaryclass = {astro-ph.EP},
	title = {{The Featureless Transmission Spectra of Two Super-puff Planets}},
	volume = {159},
	year = 2020,
	bdsk-file-1 = {},
