	date-modified = {2019-02-17 14:13:57 -0500},
	eprint = {arXiv:astro-ph/9805009},
	journal = {\aap},
	keywords = {STARS: LOW-MASS, BROWN DWARFS, STARS: EVOLUTION, STARS: HERTZSPRUNG-RUSSEL (HR) AND C-M DIAGRAMS, STARS: INTERIORS, STARS: ATMOSPHERES},
	month = sep,
	pages = {403-412},
