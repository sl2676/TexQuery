	author = {{Vorobyov}, E.~I. and {Elbakyan}, V. and {Hosokawa}, T. and {Sakurai}, Y. and {Guedel}, M. and {Yorke}, H.},
	date-added = {2019-02-17 14:13:56 -0500},
	date-modified = {2019-02-17 14:13:56 -0500},
	doi = {10.1051/0004-6361/201630356},
