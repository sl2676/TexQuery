	date-added = {2019-02-17 14:13:56 -0500},
	date-modified = {2019-02-17 14:13:57 -0500},
	doi = {10.1086/662640},
	eprint = {1109.1358},
	journal = {\pasp},
	keywords = {Astronomical Techniques},
	month = nov,
	pages = {1273},
	primaryclass = {astro-ph.EP},
	title = {{Ground-Based Submillimagnitude CCD Photometry of Bright Stars Using Snapshot Observations}},
	volume = 123,
	year = 2011,
