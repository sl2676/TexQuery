	journal = {\aap},
	keywords = {convection, stars: activity, stars: binaries: eclipsing, stars: evolution, stars: low-mass, brown dwarfs, stars: rotation},
	month = sep,
	pages = {L17-L20},
	title = {{Evolution of low-mass star and brown dwarf eclipsing binaries}},
