	journal = {\aj},
	keywords = {binaries: visual, stars: abundances, stars: fundamental parameters, stars: late-type, techniques: spectroscopic},
	month = jun,
	pages = {160},
	primaryclass = {astro-ph.SR},
	title = {{Prospecting in Ultracool Dwarfs: Measuring the Metallicities of Mid- and Late-M Dwarfs}},
	volume = 147,
	year = 2014,
	bdsk-file-1 = {},
	bdsk-url-1 = {http://dx.doi.org/10.1088/0004-6256/147/6/160},
