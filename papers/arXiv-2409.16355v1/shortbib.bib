	author = {{Gilbert}, K.~M. and {Guhathakurta}, P. and {Kalirai}, J.~S. and {Rich}, R.~M. and {Majewski}, S.~R. and {Ostheimer}, J.~C. and {Reitzel}, D.~B. and {Cenarro}, A.~J. and {Cooper}, M.~C. and {Luine}, C. and {Patterson}, R.~J.},
