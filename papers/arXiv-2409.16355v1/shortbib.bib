	doi = {10.1086/176657},
	journal = {\apj},
	keywords = {STARS: BINARIES: ECLIPSING, STARS: BINARIES: SPECTROSCOPIC, STARS: ABUNDANCES, STARS: INDIVIDUAL CONSTELLATION NAME: CM DRACONIS, STARS: POPULATION II},
	month = jan,
	pages = {356},
	title = {{The Low-Mass Double-lined Eclipsing Binary CM Draconis: A Test of the Primordial Helium Abundance and the Mass-Radius Relation near the Bottom of the Main Sequence}},
	volume = 456,
	year = 1996,
	bdsk-file-1 = {},
	bdsk-url-1 = {http://dx.doi.org/10.1086/176657},
