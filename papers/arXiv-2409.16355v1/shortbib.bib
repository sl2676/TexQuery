	doi = {10.1088/0067-0049/220/1/16},
	eid = {16},
	eprint = {1509.01565},
	journal = {\apjs},
	keywords = {stars: abundances, stars: fundamental parameters, stars: low-mass, techniques: spectroscopic},
	month = sep,
	pages = {16},
