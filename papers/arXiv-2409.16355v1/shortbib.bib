	archiveprefix = {arXiv},
	arxivurl = {http://arxiv.org/abs/1804.09365},
	author = {{Gaia Collaboration} and {Brown}, A.~G.~A. and {Vallenari}, A. and {Prusti}, T. and {de Bruijne}, J.~H.~J. and {Babusiaux}, C. and {Bailer-Jones}, C.~A.~L. and {Biermann}, M. and {Evans}, D.~W. and {Eyer}, L. and et al.},
	date-added = {2019-02-17 14:13:56 -0500},
