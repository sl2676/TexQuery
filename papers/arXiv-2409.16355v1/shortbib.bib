	archiveprefix = {arXiv},
	author = {{Covey}, K.~R. and {Hawley}, S.~L. and {Bochanski}, J.~J. and {West}, A.~A. and {Reid}, I.~N. and {Golimowski}, D.~A. and {Davenport}, J.~R.~A. and {Henry}, T. and {Uomoto}, A. and {Holtzman}, J.~A.},
	date-added = {2019-02-17 14:13:56 -0500},
