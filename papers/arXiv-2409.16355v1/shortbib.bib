	journal = {\solphys},
	keywords = {Sun: abundances, Sun: photosphere, Line: formation, Hydrodynamics, Convection, Radiative transfer},
	month = feb,
	pages = {255-269},
	primaryclass = {astro-ph.SR},
	title = {{Solar Chemical Abundances Determined with a CO5BOLD 3D Model Atmosphere}},
