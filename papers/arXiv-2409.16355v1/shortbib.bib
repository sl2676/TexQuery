	author = {{Berger}, D.~H. and {Gies}, D.~R. and {McAlister}, H.~A. and {ten Brummelaar}, T.~A. and {Henry}, T.~J. and {Sturmann}, J. and {Sturmann}, L. and {Turner}, N.~H. and {Ridgway}, S.~T. and {Aufdenberg}, J.~P. and {M{\'e}rand}, A.},
