	journal = {\apj},
	keywords = {astrometry, Galaxy: kinematics and dynamics, instrumentation: adaptive optics},
	month = dec,
	pages = {331-352},
	primaryclass = {astro-ph.GA},
	title = {{Improving Galactic Center Astrometry by Reducing the Effects of Geometric Distortion}},
