	keywords = {Stars: Binaries: Visual, Stars: Abundances, Stars: Atmospheres, stars: individual (HIP 12114), stars: individual (HIP 26907), stars: individual (HIP 32423), stars: individual (HIP 40035), stars: individual (HIP 102040), Stars: Late-Type},
