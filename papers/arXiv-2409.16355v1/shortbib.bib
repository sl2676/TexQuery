	doi = {10.1086/522831},
	eprint = {0708.2719},
	journal = {\aj},
	keywords = {open clusters and associations: individual: Praesepe: Coma Berenices, stars: evolution, stars: fundamental parameters, stars: luminosity function, mass function},
	month = dec,
