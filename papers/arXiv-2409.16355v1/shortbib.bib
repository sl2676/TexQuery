	doi = {10.1088/0004-637X/771/1/18},
	eid = {18},
	eprint = {1305.3228},
	journal = {\apj},
	keywords = {astrobiology, planetary systems, planets and satellites: formation, stars: fundamental parameters, techniques: radial velocities},
	month = jul,
	pages = {18},
	primaryclass = {astro-ph.EP},
	title = {{An Understanding of the Shoulder of Giants: Jovian Planets around Late K Dwarf Stars and the Trend with Stellar Mass}},
