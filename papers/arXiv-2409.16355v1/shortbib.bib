	date-modified = {2019-02-17 14:13:56 -0500},
	doi = {10.3847/1538-4357/aaccf5},
	eid = {53},
	eprint = {1712.05046},
	journal = {\apj},
	keywords = {binaries: eclipsing, methods: analytical, planetary systems},
	month = jul,
	pages = {53},
	primaryclass = {astro-ph.SR},
	title = {{Measuring Model-independent Masses and Radii of Single-lined Eclipsing Binaries: Analytic Precision Estimates}},
