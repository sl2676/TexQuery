	journal = {\apj},
	keywords = {Methods: n-Body Simulations, Planets and Satellites: Formation},
	month = may,
	pages = {1131-1139},
	title = {{Formation of Terrestrial Planets from Protoplanets. I. Statistics of Basic Dynamical Properties}},
	volume = 642,
	year = 2006,
