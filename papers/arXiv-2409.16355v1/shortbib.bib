	author = {{Weiss}, L.~M. and {Marcy}, G.~W. and {Rowe}, J.~F. and {Howard}, A.~W. and {Isaacson}, H. and {Fortney}, J.~J. and {Miller}, N. and {Demory}, B.-O. and {Fischer}, D.~A. and {Adams}, E.~R. and {Dupree}, A.~K. and {Howell}, S.~B. and {Kolbl}, R. and {Johnson}, J.~A. and {Horch}, E.~P. and {Everett}, M.~E. and {Fabrycky}, D.~C. and {Seager}, S.},
