	author = {{Luger}, R. and {Agol}, E. and {Kruse}, E. and {Barnes}, R. and {Becker}, A. and {Foreman-Mackey}, D. and {Deming}, D.},
	date-added = {2019-02-17 14:13:56 -0500},
	date-modified = {2019-02-17 14:13:56 -0500},
	doi = {10.3847/0004-6256/152/4/100},
	eid = {100},
