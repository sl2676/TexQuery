	eprint = {arXiv:astro-ph/0505432},
	journal = {\aap},
	keywords = {space vehicles: instruments, astrometry},
	month = aug,
	pages = {791},
	title = {{A new reduction of the raw Hipparcos data}},
	volume = 439,
	year = 2005,
