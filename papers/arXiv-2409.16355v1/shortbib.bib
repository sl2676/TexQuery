	doi = {10.1111/j.1365-2966.2011.20170.x},
	eprint = {1111.2396},
	journal = {\mnras},
	keywords = {galaxies: clusters: general, cosmology: observations, X-rays: galaxies: clusters},
	month = mar,
	pages = {2120-2138},
	primaryclass = {astro-ph.CO},
	title = {{X-ray-optical classification of cluster mergers and the evolution of the cluster merger fraction}},
