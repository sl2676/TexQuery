	journal = {\apj},
	keywords = {stars: activity, stars: atmospheres, stars: evolution, stars: interiors, stars: low-mass},
	month = oct,
	pages = {87},
	primaryclass = {astro-ph.SR},
	title = {{The Radius Discrepancy in Low-mass Stars: Single versus Binaries}},
	volume = 776,
	year = 2013,
	bdsk-file-1 = {},
	bdsk-url-1 = {http://dx.doi.org/10.1088/0004-637X/776/2/87},
