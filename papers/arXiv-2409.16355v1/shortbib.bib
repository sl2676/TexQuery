	eprint = {0708.0243},
	journal = {\pasp},
	keywords = {Extrasolar Planets},
	month = sep,
	pages = {986-993},
	title = {{Effects of Orbital Eccentricity on Extrasolar Planet Transit Detectability and Light Curves}},
	volume = 119,
