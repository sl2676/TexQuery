	date-added = {2019-02-17 14:13:56 -0500},
	date-modified = {2019-02-17 14:13:57 -0500},
	doi = {10.1088/0004-637X/719/2/1454},
	eprint = {1007.2881},
	journal = {\apj},
	keywords = {celestial mechanics, planet-disk interactions, planetary systems, planets and satellites: dynamical evolution and stability, planets and satellites: formation},
