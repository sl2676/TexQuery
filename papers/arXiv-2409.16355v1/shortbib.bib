	arxivurl = {http://arXiv.org/abs/astro-ph/0608099},
	author = {{Bayless}, A.~J. and {Orosz}, J.~A.},
	date-added = {2019-02-17 14:13:56 -0500},
	date-modified = {2019-02-17 14:13:56 -0500},
