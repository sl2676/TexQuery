	journal = {\apj},
	keywords = {stars: activity, stars: chromospheres, stars: kinematics and dynamics, stars: late-type, stars: low-mass, stars: rotation},
	month = oct,
	pages = {3},
	primaryclass = {astro-ph.SR},
	title = {{An Activity-Rotation Relationship and Kinematic Analysis of Nearby Mid-to-Late-Type M Dwarfs}},
