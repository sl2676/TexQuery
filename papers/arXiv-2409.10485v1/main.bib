	volume = {673},
	issn = {0004-6361, 1432-0746},
	shorttitle = {{StarHorse} results for spectroscopic surveys and {Gaia} {DR3}},
	url = {https://www.aanda.org/articles/aa/abs/2023/05/aa45399-22/aa45399-22.html},
