	year = {1976},
	note = {ADS Bibcode: 1976A\&A....48..301M},
	keywords = {Astronomical Spectroscopy, Astrophysics, Chemical Composition, Dwarf Stars, F Stars, G Stars, Galactic Evolution, Galactic Structure, K Stars, Stellar Structure, Ubv Spectra},
	pages = {301--315},
}

