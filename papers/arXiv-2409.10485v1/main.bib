	journal = {The Astronomical Journal},
	author = {Cañas, Caleb I. and Mahadevan, Suvrath and Bender, Chad F. and Rivera, Noah Isaac Salazar and Monson, Andrew and Beard, Corey and Lubin, Jack and Robertson, Paul and Gupta, Arvind F. and Cochran, William D. and Fredrick, Connor and Hearty, Fred and Jones, Sinclaire and Kanodia, Shubham and Lin, Andrea S. J. and Ninan, Joe P. and Ramsey, Lawrence W. and Schwab, Christian and Stefánsson, Guðmundur},
