@article{wang_hot_2020,
	title = {Hot {Jupiter} and {Ultra}-cold {Saturn} {Formation} in {Dense} {Star} {Clusters}},
	volume = {905},
	issn = {0004-637X},
	url = {https://ui.adsabs.harvard.edu/abs/2020ApJ...905..136W},
	doi = {10.3847/1538-4357/abc619},
	abstract = {The discovery of high incidence of hot Jupiters in dense clusters challenges the field-based hot Jupiter formation theory. In dense clusters, interactions between planetary systems and flyby stars are relatively common. This has a significant impact on planetary systems, dominating hot Jupiter formation. In this paper, we perform high precision, few-body simulations of stellar flybys and subsequent planet migration in clusters. A large parameter space exploration demonstrates that close flybys that change the architecture of the planetary system can activate high eccentricity migration mechanisms: LK and planet-planet scattering, leading to high hot Jupiter formation rate in dense clusters. Our simulations predict that many of the hot Jupiters are accompanied by "ultra-cold Saturns," expelled to apastra of thousands of astronomical units. This increase is particularly remarkable for planetary systems originally hosting two giant planets with semimajor axis ratios of ∼4 and the flyby star approaching nearly perpendicular to the planetary orbital plane. The estimated lower limit to the hot Jupiter formation rate of a virialized cluster is \${\textbackslash}sim 1.6{\textbackslash}times \{10\}{\textasciicircum}\{-4\}\{({\textbackslash}sigma /1{\textbackslash}mathrm\{km\}\{\{{\textbackslash}rm\{s\}\}\}{\textasciicircum}\{-1\})\}{\textasciicircum}\{5\}\{(\{a\}\_\{\{{\textbackslash}rm\{p\}\}\}/20{\textbackslash}mathrm\{au\})(\{M\}\_\{\{{\textbackslash}rm\{c\}\}\}/1000\{M\}\_\{{\textbackslash}odot \})\}{\textasciicircum}\{-2\}\$ Gyr-1 per star, where σ is the cluster velocity dispersion, ap is the size of the planetary system, and Mc is the mass of the cluster. Our simulations yield a hot Jupiter abundance that is ∼50 times smaller than that observed in the old open cluster M67. We expect that interactions involving binary stars, as well as a third or more giant planets, will close the discrepancy.},
	urldate = {2024-08-27},
	journal = {The Astrophysical Journal},
	author = {Wang, Yi-Han and Leigh, Nathan W. C. and Perna, Rosalba and Shara, Michael M.},
	month = dec,
	year = {2020},
	note = {Publisher: IOP
ADS Bibcode: 2020ApJ...905..136W},
	keywords = {1083, 1255, 1257, 1596, 753, Astrophysics - Astrophysics of Galaxies, Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Solar and Stellar Astrophysics, Hot Jupiters, N-body simulations, Planetary science, Planetary system formation, Stellar dynamics},
	pages = {136},
}

@article{rodet_correlation_2021,
	title = {On the {Correlation} between {Hot} {Jupiters} and {Stellar} {Clustering}: {High}-eccentricity {Migration} {Induced} by {Stellar} {Flybys}},
	volume = {913},
	issn = {0004-637X},
	shorttitle = {On the {Correlation} between {Hot} {Jupiters} and {Stellar} {Clustering}},
	url = {https://dx.doi.org/10.3847/1538-4357/abf8a7},
	doi = {10.3847/1538-4357/abf8a7},
	abstract = {A recent observational study suggests that the occurrence of hot Jupiters (HJs) around solar-type stars is correlated with stellar clustering. We study a new scenario for HJ formation, called “Flyby Induced High-e Migration,” that may help explain this correlation. In this scenario, stellar flybys excite the eccentricity and inclination of an outer companion (giant planet, brown dwarf, or low-mass star) at large distance (10–300 au), which then triggers high-e migration of an inner cold Jupiter (at a few astronomical units) through the combined effects of von Zeipel–Lidov–Kozai (ZLK) eccentricity oscillation and tidal dissipation. Using semianalytical calculations of the effective ZLK inclination window, together with numerical simulations of stellar flybys, we obtain the analytic estimate for the HJ occurrence rate in this formation scenario. We find that this “flyby induced high-e migration” could account for a significant fraction of the observed HJ population, although the result depends on several uncertain parameters, including the density and lifetime of birth stellar clusters, and the occurrence rate of the “cold Jupiter + outer companion” systems.},
	language = {en},
	number = {2},
	urldate = {2024-08-27},
	journal = {The Astrophysical Journal},
	author = {Rodet, Laetitia and Su, Yubo and Lai, Dong},
	month = jun,
	year = {2021},
	note = {Publisher: The American Astronomical Society},
	pages = {104},
}

