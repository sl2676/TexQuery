@article{canas_eccentric_2022,
	title = {An eccentric {Brown} {Dwarf} eclipsing an {M} dwarf},
	volume = {163},
	issn = {0004-6256, 1538-3881},
	url = {http://arxiv.org/abs/2112.03959},
	doi = {10.3847/1538-3881/ac415f},
	abstract = {We report the discovery of a M = 67 ± 2MJ brown dwarf transiting the early M dwarf TOI-2119 on an eccentric orbit (e = 0.3362 ± 0.0005) at an orbital period of 7.200861 ± 0.000005 days. We conﬁrm the brown dwarf nature of the transiting companion using a combination of ground-based and space-based photometry and highprecision velocimetry from the Habitable-zone Planet Finder. Detection of the secondary eclipse with TESS photometry enables a precise determination of the eccentricity and reveals the brown dwarf has a brightness temperature of 2100 ± 80 K, a value which is consistent with an early L dwarf. TOI-2119 is one of the most eccentric known brown dwarfs with P {\textless} 10 days, possibly due to the long circularization timescales for an object orbiting an M dwarf. We assess the prospects for determining the obliquity of the host star to probe formation scenarios and the possibility of additional companions in the system using Gaia EDR3 and our radial velocities.},
	language = {en},
	number = {2},
